{
  "name": "Unirideng",
  "short_name": "Uniride",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "description": "Uniride. Nigeria's premier student ride-sharing platform connecting universities to cities across the country.",
  "icons": [
    {
      "src": "/favicon-48x48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "orientation": "portrait",
  "lang": "en",
  "screenshots": [
    {
      "src": "/screenshot-wide.png",
      "sizes": "1200x630",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Main app screen"
    },
    {
      "src": "/screenshot-narrow.png",
      "sizes": "540x960",
      "type": "image/png",
      "label": "App mobile screen"
    }
  ],
  "id": "/",
  "protocol_handlers": [
    {
      "protocol": "web+uniride",
      "url": "/?handler=%s"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone", "browser"]
}
